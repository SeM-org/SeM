\module{Logic}

\begin[True(Binary)]{def}
\alias{One}
\end{def}

\begin[False(Binary)]{def}
\synonim{Zero}
\end{def}

\begin[bool]{def}
\alias{Bool}
\end{def}

\begin[Relation]{def}
\return{Bool}
\end{def}

\begin[BinaryRelation:Relation]{def}
\parameters{Two}
\end{def}

\begin[Equality:BinaryRelation]{def}
\parameter[lhs]{set}
\parameter[rhs]{set}
\begin[=]{symbol}
\left{\lhs}
\right{\rhs}
\end{symbol}
\alias{==}
\lhs\is\rhs
\end{def}
