\begin[Order:BinaryRelation]{def}
\parameter[smaller]{set}
\parameter[bigger]{set}
\begin[<]{symbol}
\left{\smaller}
\right{\bigger}
\end{symbol}
\begin[>]{symbol}
\left{\bigger}
\right{\smaller}
\end{symbol}
